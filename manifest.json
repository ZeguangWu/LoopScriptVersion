{
  "manifest_version": 2,
  "name": "Loop Script Version",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://res-sdf.cdn.office.net/*",
    "https://loop.cloud-dev.microsoft/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://loop.cloud-dev.microsoft/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
